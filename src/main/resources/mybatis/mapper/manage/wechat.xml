<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ailu.tokenmedia.manage.wechat.dao.WechatDao">

    <update id="updataByWechatid">
        UPDATE wechat
        SET biz_name = #{bizName},
        head = #{head},
        biz_desc = #{bizDes}
        WHERE wechat_id = #{wechatid}
    </update>

</mapper>